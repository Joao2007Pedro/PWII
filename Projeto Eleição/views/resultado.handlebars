<main>
    <div class="container mt-5">
        <h1>Resultados da Eleição</h1>
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>Nome</th>
                    <th>Votos</th>
                </tr>
            </thead>
            <tbody>
                {{#each candidatos}}
                    <tr>
                        <td>{{nome}}</td>
                        <td>{{votos}}</td>
                    </tr>
                {{/each}}
            </tbody>
        </table>
        <a href="/index" class="btn btn-primary">Voltar ao início</a>
    </div>
</main>
